{
  "name": "workout-logs",
  "version": "1.0.0",
  "description": "A workout tracking application with user authentication and progress visualization",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "client": "cd client && npm run dev",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "populate-db": "node server/scripts/populateDb.js"
  },
  "keywords": [
    "workout",
    "fitness",
    "tracking",
    "exercise"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "react-icons": "^5.4.0",
    "react-native-safe-area-context": "^5.2.0",
    "react-native-screens": "^4.6.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1"
  }
}
